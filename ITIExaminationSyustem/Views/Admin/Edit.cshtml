@model Admin
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<form asp-action="Update" asp-controller="Admin" method="post">
    <input type="hidden" asp-for="@Model.Admin_Id" />
    <div class="form-group">
        <label asp-for="@Model.Navigation_User.User_Email"></label>
        <select asp-for="Admin_User_Id" class="form-control" asp-items="@(new SelectList(ViewBag.Emaillist, "Admin_User_Id", "User_Email"))">
        </select>
    </div>
    <div class="form-group">
        <label asp-for="@Model.Navigation_Branch.Branch_Name"></label>
        <select asp-for="Admin_Branch_Id" class="form-control" asp-items="@(new SelectList( ViewBag.Branchlist, "Admin_Branch_Id", "Branch_Name"))">
        </select>
    </div>

    <input type="submit" value="Save" class="btn btn-danger" />
</form>

<a asp-action="Index" asp-controller="admin" class="btn btn-info">BackToList</a>