@model IEnumerable<BranchManager>
@{
    ViewData["Title"] = "Index";
}

<h1>All Managers</h1>

<table class=" table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>Manager Name</th>
            <th>Actions</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Branch_Manager_Name</td>
                <td>
                    <a asp-action="Details" asp-controller="BranchManager" class="btn btn-info">Details</a>
                    <a asp-action="Edit" asp-controller="BranchManager" class="btn btn-info">Edit</a>
                    <a asp-action="Delete" asp-controller="BranchManager" class="btn btn-danger">Delete</a>
                </td>
            </tr>
        }
    </thead>
    <tbody>

        <a asp-action="AddBranch" asp-controller="BranchManager" class="btn btn-info">Add</a>
