@model ITIExaminationSyustem.Models.Course

@{
    ViewData["Title"] = "CourseDetails";
}

<h1>Course Details</h1>
<div>
    <table class="table table-striped table-hover">
        <tbody>
            <tr>
                <th>ID</th>
                <td>@Model.Course_Id</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>@Model.Course_Name</td>
            </tr>
            <tr>
                <th>Duration</th>
                <td>@Model.Course_Duration</td>
            </tr>
            <tr>
                <th>Exam Duration</th>
                <td>@Model.Crs_Exam_Duration</td>
            </tr>
            <tr>
                <th>Exam Start Date</th>
                <td>@Model.Course_Exam_StartDate</td>
            </tr>
        </tbody>
    </table>
</div>